<h1 class="page-title txt-color-blueDark"><i class="fa-fw fa fa-building"></i> Edificios</h1>
<div class="panel panel-default">
    <div class="panel-body">
        <input id="hdIdSede" type="hidden" />

        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label " for="txtCodigoFiltro">Código</label>
                    <input type="text" id="txtCodigoFiltro" name="txtCodigoFiltro" class="form-control input-sm input-outer-trim habilitadoModifica numero" maxlength="10">
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label " for="txtNombreFiltro">Nombre</label>
                    <input type="text" id="txtNombreFiltro" name="txtNombreFiltro" class="form-control input-sm input-outer-trim habilitadoModifica" maxlength="100">
                </div>
            </div>
            <div class="col-md-3">
                    <div class="form-group">
                        <label class="control-label">Sede</label>
                        <select id="cmbSedeFiltro" name="cmbSedeFiltro" class="form-control input-sm"></select>
                    </div>
                </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label">Vigente</label>
                    <div class="boton-switch">
                        <input id="chkVigenteFiltro" name="chkVigenteFiltro" type="checkbox" class="switchery habilitadoModifica" data-theme="blue" checked disabled />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel-footer clearfix">
        <div class="row">
            <div class="col-xs-12 col-md-2 col-md-offset-8">
                <button id="btnBuscar" type="button" name="btnBuscar" class="btn btn-round btn-sm btn-primary btn-block ">
                    <i class="glyphicon glyphicon-search"></i>
                    Buscar
                </button>
            </div>
            <div class="col-xs-12 col-md-2">
                <button id="btnNuevo" type="button" name="btnNuevo" class="btn btn-round btn-sm btn-primary btn-block ">
                    <i class="fa fa-lg fa-fw fa-plus"></i>
                    Nuevo
                </button>
            </div>
        </div>
    </div>
</div>

<div class="panel panel-default clearfix">
    <div class="panel-body">
        <div class="row">
            <div class="col-md-12">
                <table id="tbEdificios" class="dataTableFull table table-condensed table-striped table-bordered nowrap display" cellspacing="0">
                    <thead>
                        <tr>
                            <th class="hidden-lg hidden-md hidden-sm visible-xs firtscell"></th>
                            <th></th>
                            <th></th>
                            <th>CÓDIGO</th>
                            <th>NOMBRE EDIFICIO</th>
                            <th>SEDE</th>
                            <th>FECHA CREACIÓN</th>
                            <th>FECHA MODIFICACIÓN</th>
                            <th>VIGENTE</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>
