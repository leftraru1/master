<h1 class="page-title txt-color-blueDark"><i class="fa-fw fa fa-male"></i> Usuarios <span> > Registro</span></h1>
<div class="panel panel-default">
    <div class="panel-body">
        <form id="FormTrabajador">
            
            <input id="hdIdUsuario" type="hidden" />
            <input id="hdDataUsuario" type="hidden" />

            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="control-label" for="txtRutCompleto">RUT</label>
                        <div class="input-group">
                            <input type="text" id="txtRutCompleto" name="txtRutCompleto" class="form-control input-sm rut habilitadoInicial" aria-describedby="basic-addon1" maxlength="12">
                            <span class="input-group-btn">
                                <button id="btnBuscarRut" name="btnBuscarRut" type="button" class="btn btn-sm btn-primary btn-buscar habilitadoInicial"><span class="fa fa-search"></span></button>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="control-label">Perfil</label>
                        <select id="cmbPerfil" name="cmbPerfil" class="form-control input-sm habilitadoModifica"></select>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="control-label">Sede</label>
                        <select id="cmbSede" name="cmbSede" class="form-control input-sm habilitadoModifica"></select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="control-label">Nombre completo</label>
                        <input type="text" id="txtNombreCompleto" name="txtNombreCompleto" class="form-control input-sm input-outer-trim habilitadoModifica" maxlength="100">
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="control-label">Login</label>
                        <input type="text" id="txtLogin" name="txtLogin" class="form-control input-sm input-outer-trim habilitadoModifica" maxlength="50">
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="control-label ">Vigente</label>
                        <div class="boton-switch">
                            <input id="chkVigente" name="chkVigente" type="checkbox" class="switchery habilitadoModifica" data-theme="blue" checked disabled />
                        </div>
                    </div>
                </div>
            </div>

        </form>
    </div>
</div>
<div class="panel panel-default">
    <div class="panel-footer clearfix">
        <div class="row">
            <div class="col-xs-12 col-md-2 col-md-offset-6">
                <button id="btnGuardar" type="button" name="btnGuardar" class="btn btn-round btn-sm btn-primary btn-block habilitadoModifica">
                    <i class="glyphicon glyphicon-save"></i>
                    Guardar
                </button>
            </div>
            <div class="col-xs-12 col-md-2">
                <button id="btnLimpiar" type="button" class="btn btn-sm btn-block btn-round btn-primary habilitadoModifica">
                    <i class="fa fa-lg fa-fw fa-recycle"></i>
                    Limpiar
                </button>
            </div>
            <div class="col-xs-12 col-md-2">
                <button id="btnVolver" type="button" name="btnVolver" class="btn btn-round btn-sm btn-danger btn-block ">
                    <i class="glyphicon glyphicon-chevron-left"></i>
                    Volver
                </button>
            </div>
        </div>
    </div>
</div>